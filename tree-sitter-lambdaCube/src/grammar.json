{
  "name": "lambdaCube",
  "rules": {
    "expr": {
      "type": "SYMBOL",
      "name": "_expr"
    },
    "_comment": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "comment_line"
        },
        {
          "type": "SYMBOL",
          "name": "comment_block"
        }
      ]
    },
    "comment_line": {
      "type": "PATTERN",
      "value": "-- [^\\n]*"
    },
    "comment_block": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{- "
        },
        {
          "type": "SYMBOL",
          "name": "_end_comment"
        }
      ]
    },
    "_end_comment": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": " -}"
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_comment"
            },
            {
              "type": "SYMBOL",
              "name": "_end_comment"
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "PATTERN",
              "value": "\\s|\\S"
            },
            {
              "type": "SYMBOL",
              "name": "_end_comment"
            }
          ]
        }
      ]
    },
    "sym": {
      "type": "PATTERN",
      "value": "[a-zA-Z]\\w*"
    },
    "nat": {
      "type": "PATTERN",
      "value": "\\d+"
    },
    "level_t": {
      "type": "STRING",
      "value": "#L"
    },
    "_levels": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_level"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": ","
              },
              {
                "type": "SYMBOL",
                "name": "_level"
              }
            ]
          }
        }
      ]
    },
    "_level": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "sym"
        },
        {
          "type": "SYMBOL",
          "name": "level_add"
        },
        {
          "type": "SYMBOL",
          "name": "nat"
        }
      ]
    },
    "level_add": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "sym"
        },
        {
          "type": "STRING",
          "value": "+"
        },
        {
          "type": "SYMBOL",
          "name": "nat"
        }
      ]
    },
    "universe": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "#U"
        },
        {
          "type": "SYMBOL",
          "name": "_levels"
        }
      ]
    },
    "named": {
      "type": "PREC_RIGHT",
      "value": 1,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "@"
          },
          {
            "type": "FIELD",
            "name": "name",
            "content": {
              "type": "SYMBOL",
              "name": "sym"
            }
          },
          {
            "type": "STRING",
            "value": " = "
          },
          {
            "type": "FIELD",
            "name": "body",
            "content": {
              "type": "SYMBOL",
              "name": "expr"
            }
          },
          {
            "type": "STRING",
            "value": ";"
          },
          {
            "type": "FIELD",
            "name": "rest",
            "content": {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "expr"
                },
                {
                  "type": "BLANK"
                }
              ]
            }
          }
        ]
      }
    },
    "bracket": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "["
        },
        {
          "type": "SYMBOL",
          "name": "_expr"
        },
        {
          "type": "STRING",
          "value": "]"
        }
      ]
    },
    "_base_expr": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "named"
        },
        {
          "type": "SYMBOL",
          "name": "sym"
        },
        {
          "type": "SYMBOL",
          "name": "nat"
        },
        {
          "type": "SYMBOL",
          "name": "universe"
        },
        {
          "type": "SYMBOL",
          "name": "level_t"
        }
      ]
    },
    "_pre_expr": {
      "type": "PREC_RIGHT",
      "value": 3,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "SYMBOL",
            "name": "_comment"
          },
          {
            "type": "SYMBOL",
            "name": "_expr"
          }
        ]
      }
    },
    "_post_expr": {
      "type": "PREC_LEFT",
      "value": 2,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "SYMBOL",
            "name": "_expr"
          },
          {
            "type": "SYMBOL",
            "name": "_comment"
          }
        ]
      }
    },
    "_expr": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "bracket"
        },
        {
          "type": "SYMBOL",
          "name": "_comment"
        },
        {
          "type": "SYMBOL",
          "name": "_base_expr"
        },
        {
          "type": "SYMBOL",
          "name": "_pre_expr"
        },
        {
          "type": "SYMBOL",
          "name": "_post_expr"
        }
      ]
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}
